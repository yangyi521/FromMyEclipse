<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<servlet>
		<servlet-name>Check</servlet-name>
		<servlet-class>Servlet.Check</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>adminDal</servlet-name>
		<servlet-class>Servlet.adminDal</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>memberDal</servlet-name>
		<servlet-class>Servlet.memberDal</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>ReturnBook</servlet-name>
		<servlet-class>Servlet.ReturnBook</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>bookDal</servlet-name>
		<servlet-class>Servlet.bookDal</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>LoanLogDal</servlet-name>
		<servlet-class>Servlet.LoanLogDal</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>ReturnBook</servlet-name>
		<url-pattern>/ReturnBook</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>bookDal</servlet-name>
		<url-pattern>/bookDal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Check</servlet-name>
		<url-pattern>/Check</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>adminDal</servlet-name>
		<url-pattern>/adminDal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>memberDal</servlet-name>
		<url-pattern>/memberDal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoanLogDal</servlet-name>
		<url-pattern>/LoanLogDal</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

</web-app>
